<idea-plugin>
    <id>org.virtuslab.ideprobe.pants</id>
    <name>Virtuslab IdeProbe Pants</name>
    <vendor email="info@virtuslab.com" url="http://www.virtuslab.com">VirtusLab</vendor>

    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.java</depends>
    <depends>org.virtuslab.ideprobe</depends>
    <depends>com.intellij.plugins.pants</depends>
    <idea-version since-build="191.*" until-build="201.*"/>

    <extensions defaultExtensionNs="org.virtuslab.ideprobe">
        <probeHandlerContributor implementation="com.twitter.intellij.pants.probe.PantsProbeHandlerContributor"/>
    </extensions>
</idea-plugin>
